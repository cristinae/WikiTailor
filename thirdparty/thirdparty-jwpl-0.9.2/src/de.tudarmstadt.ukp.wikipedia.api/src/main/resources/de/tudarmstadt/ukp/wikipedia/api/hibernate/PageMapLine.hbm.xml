<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.tudarmstadt.ukp.wikipedia.api.hibernate.PageMapLine" table="PageMapLine">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name" index="name_index"/>
        <property name="pageID" />
        <property name="stem" />
        <property name="lemma" />
    </class>
</hibernate-mapping>