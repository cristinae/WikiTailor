<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.tudarmstadt.ukp.wikipedia.api.hibernate.MetaData" table="MetaData">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="language" />
        <property name="disambiguationCategory" />
        <property name="mainCategory" />
        <property name="nrofPages" />
        <property name="nrofRedirects" />
        <property name="nrofDisambiguationPages" />
        <property name="nrofCategories" />
        <property name="version" />
    </class>
</hibernate-mapping>