<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.tudarmstadt.ukp.wikipedia.api.hibernate.RelatednessCacheLine" table="RelatednessCacheLine">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="page1" index="pagePairIndex" />
        <property name="page2" index="pagePairIndex" />

		<property name="PathLengthAverage" />
