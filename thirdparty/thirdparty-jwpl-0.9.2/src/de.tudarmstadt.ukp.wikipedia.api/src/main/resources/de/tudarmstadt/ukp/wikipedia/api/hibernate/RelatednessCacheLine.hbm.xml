		<property name="LeacockChodorowAverage" />
		<property name="LeacockChodorowBest" />
		<property name="LeacockChodorowSelectivity" />
		<property name="ResnikAverage" />
		<property name="ResnikBest" />
		<property name="ResnikSelectivityLinear" />
		<property name="ResnikSelectivityLog" />
		<property name="LinAverage" />
		<property name="LinBest" />
		<property name="JiangConrathAverage" />
		<property name="JiangConrathBest" />
		<property name="WuPalmerAverage" />
		<property name="WuPalmerBest" />
		<property name="LeskFirst" />
		<property name="LeskFull" />

    </class>
</hibernate-mapping>